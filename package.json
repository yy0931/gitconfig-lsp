{
  "name": "gitconfig-lsp",
  "displayName": "git-config Language Server",
  "version": "0.3.0",
  "publisher": "yy0931",
  "description": "",
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yy0931/gitconfig-lsp"
  },
  "bugs": {
    "url": "https://github.com/yy0931/gitconfig-lsp/issues"
  },
  "scripts": {
    "build": "esbuild --sourcemap=external --target=esnext --platform=node --format=cjs --outdir=dist src/*.ts",
    "generate-docs": "npm run build && node dist/generate-docs.js",
    "prepare": "npm run generate-docs",
    "vscode:prepublish": "npm run generate-docs",
    "test": "npm run build && mocha dist/*.test.js"
  },
  "activationEvents": [
    "onLanguage:gitconfig",
    "onLanguage:properties",
    "onLanguage:git-internals",
    "onCustomEditor:gitconfig-lsp.cat-file",
    "onCustomEditor:gitconfig-lsp.ls-files--stage",
    "onCustomEditor:gitconfig-lsp.verify-pack-v"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "preview": true,
  "contributes": {
    "customEditors": [
      {
        "viewType": "gitconfig-lsp.cat-file",
        "displayName": "git cat-file",
        "selector": [
          {
            "filenamePattern": "**/.git/objects/??/??????????????????????????????????????"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "gitconfig-lsp.ls-files--stage",
        "displayName": "git ls-files --stage",
        "selector": [
          {
            "filenamePattern": "**/.git/index"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "gitconfig-lsp.verify-pack-v",
        "displayName": "git verify-pack -v",
        "selector": [
          {
            "filenamePattern": "**/.git/objects/pack/pack-*.idx"
          },
          {
            "filenamePattern": "**/.git/objects/pack/pack-*.pack"
          }
        ],
        "priority": "default"
      }
    ],
    "languages": [
      {
        "id": "gitconfig",
        "extensions": [
          ".gitconfig",
          ".lfsconfig"
        ],
        "filenamePatterns": [
          ".gitconfig",
          ".lfsconfig",
          "config.worktree"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "git-internals",
        "filenamePatterns": [
          "**/.git/logs/**/*",
          "**/.git/refs/**/*",
          "**/.git/ORIG_HEAD",
          "**/.git/HEAD",
          "**/.git/FETCH_HEAD",
          "**/.git/packed-refs",
          "**/.git/shallow"
        ]
      }
    ],
    "grammars": [
      {
        "language": "git-internals",
        "scopeName": "source.git-internals",
        "path": "./syntaxes/git-internals.tmLanguage.json"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "gitconfigBool": [
            "constant.language.boolean.gitconfig"
          ],
          "gitconfigColor": [
            "support.constant.color.gitconfig"
          ],
          "gitconfigProperty": [
            "variable.other.constant.object.gitconfig",
            "variable.other.constant",
            "variable.other.enummember"
          ],
          "gitconfigSection": [
            "entity.name.tag.gitconfig",
            "meta.selector.gitconfig"
          ]
        }
      }
    ]
  },
  "keywords": [
    "git",
    "gitconfig",
    "lfsconfig",
    ".gitconfig",
    ".lfsconfig"
  ],
  "author": "",
  "license": "GPLv2",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.0.0",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.11.9",
    "@types/node-fetch": "^2.5.12",
    "@types/turndown": "^5.0.1",
    "@types/vscode": "^1.60.0",
    "chai": "^4.3.4",
    "esbuild": "^0.14.7",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.3",
    "node-fetch": "^2.6.6",
    "turndown": "^7.1.1"
  },
  "dependencies": {
    "mustache": "^4.2.0",
    "peggy": "^1.2.0",
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.3",
    "vscode-uri": "^3.0.3"
  }
}
